@inject school.Data.ApplicationDbContext _context

<MudButton @onclick="OpenDialog" Variant="Variant.Filled" Color="Color.Primary">
    Lägg till
</MudButton>
<MudTable items="@tentor.Take(2)" Hover="true" Breakpoint="Breakpoint.Sm"> 
    <HeaderContent>
        <MudTh>Datum</MudTh>
        <MudTh>Examinator</MudTh>
        <MudTh></MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd>@context.Datum</MudTd>
        <MudTd>@context.Examinator</MudTd>
        <MudTd>Visa</MudTd>
    </RowTemplate>
</MudTable>


@code {

    private void OpenDialog()
    {
        DialogService.Show<SkapaTenta>("Skapa tenta");
    }

    protected override async Task<Tentor> OnInitializedAsync() {
        
    }
}
