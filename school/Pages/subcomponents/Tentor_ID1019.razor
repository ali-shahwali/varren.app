@inject school.Data.ApplicationDbContext _context

<MudButton @onclick="OpenDialog" Variant="Variant.Filled" Color="Color.Primary">
    Hello
</MudButton>
<MudTable items="tentor" Hover="true" Breakpoint="Breakpoint.Sm">
    <HeaderContent>
        <MudTh>Datum</MudTh>
        <MudTh>Examinator</MudTh>
        <MudTh></MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd>@context.Datum</MudTd>
        <MudTd>@context.Examinator</MudTd>
        <MudTd>Visa</MudTd>
    </RowTemplate>
</MudTable>


@code {
    List<Tentor> tentor = new List<Tentor>();

    private void OpenDialog()
    {
        DialogService.Show<SkapaTenta>("Skapa tenta");
    }

    protected override async Task OnInitializedAsync() {
        tentor = _context.Tentor.ToList();
        
    }
}
